<div class="container-flex">
  <div class="row show-header">
    <h1>
      Want
    </h1>
  </div>
  <div class="col-md-12">
    <div class="row table col-md-12">
      <table data-toggle="table">
        <thead class="table-header">
        <tr>
          <th class="col-md-3 text-center table-head-label">Skill</th>
          <th class="col-md-6 text-center table-head-label">Description</th>
          <th class="col-md-2 text-center table-head-label">Full Info</th>
        </tr>
        </thead>
        <% @have.each do |f| %>
          <tbody>
          <tr class="text-center">
            <td class="table-body-box"><%= f.title %></td>
            <td class="table-body-box"><%= f.description %></td>
            <td class="table-body-box"><%= link_to "Info", root_path, :class => "b3" %></td>
          </tr><% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="row have-link-box col-md-offset-1">
    <h3 class="have-links">
      <%= link_to "Add New", new_want_path, :class => "b5" %>
    </h3>
  </div>
</div>
</div>